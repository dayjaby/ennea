<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="mmorpg" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release">
				<Option output="bin/Release/mmorpg" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="$(#lua.include)" />
		</Compiler>
		<Linker>
			<Add library="lua5.1" />
			<Add library="luabind" />
			<Add directory="$(#lua.lib)" />
		</Linker>
		<Unit filename="lua/containers.lua" />
		<Unit filename="lua/core.lua" />
		<Unit filename="lua/items.lua" />
		<Unit filename="lua/objects.lua" />
		<Unit filename="lua/potions.lua" />
		<Unit filename="main.cpp" />
		<Unit filename="objects.hpp" />
		<Unit filename="objects.lua" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
